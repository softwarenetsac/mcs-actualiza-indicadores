	pdi_do.apellido_paterno+' '+pdi_do.apellido_materno+' '+pdi_do.nombres as 	PDI_DEFENSOR,
	year(pdi.fecha)                                                        as 	PDI_FECHA_ANIO,
	month(pdi.fecha)                                                       as 	PDI_FECHA_MESNUM,
	(
	CASE month(pdi.fecha) 
