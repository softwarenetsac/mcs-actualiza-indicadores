	as PDI_GRUPOSERVICIO_ID,
    null as FECHA_CIERRE	 
from
	dbo.diligencia pdi_d 
		inner join dbo.patrocinio_diligencia pdi 
		on pdi_d.diligencia_id = pdi.diligencia_id 
			inner join dbo.v_listadopersonal pdi_do 
			on pdi_do.personal_id = pdi.defensor_id 
				inner join dbo.tipo_ficha pdi_tf 
				on pdi_tf.tipoficha_id = pdi.tipoficha_id 
					inner join dbo.patrocinado pdo 
					on pdo.patrocinado_id = pdi.patrocinado_id 
						inner join dbo.v_sexo pdo_sx 
						on pdo_sx.sexo_id = pdo.sexo 
							inner join dbo.situacion_juridica pdi_sj 
							on pdi_sj.situacionjuridica_id = pdi.
							situacionjuridica_id 
								inner join dbo.tipo_diligencia pdi_td 
								on pdi_td.tipodiligencia_id = pdi.
								tipodiligencia_id 
									inner join dbo.etapa_procesal pdi_ep 
									on pdi_ep.etapaprocesal_id = pdi.
									etapaprocesal_id 
										inner join dbo.distritojudicial pdi_dj 
										on pdi_dj.distritojudicial_id = pdi.
										distritojudicial_id 
											inner join dbo.sede pdi_s 
											on pdi_s.sede_id = pdi.sede_id 
												inner join dbo.ubicacion pdi_u 
												on pdi_u.ubicacion_id = pdi.
												ubicacion_id 
													inner join dbo.
													origenusuario_diligencia 
													pdi_or 
													on pdi_or.
													origenusuariodiligencia_id = 
													pdi.
													origenusuariodiligencia_id 
														inner join dbo.
														diligencia_resultado drs 
														on pdi.
														patrociniodiligencia_id 
														= drs.
														patrociniodiligencia_id 
															inner join dbo.
															resultado drs_r 
															on drs_r.
															resultado_id = drs.
															resultado_id 
where
	(pdi.diligencia_libre ='3') and
	(pdi.vigente ='S') and
	(drs.es_activo ='S') and
	(pdi.fecha >= ${FECHA_INICIO})
    ----(pdi.fecha>= '2021-01-01' )
</sql>
