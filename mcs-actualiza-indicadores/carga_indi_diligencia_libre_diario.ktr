    <sql>select
	pdi.patrociniodiligencia_id                                            as 	PDI_COD,
	pdo.apellido_paterno+' '+pdo.apellido_materno+' '+pdo.nombres          as 	PDI_PATROCINADO,
	
	pdo_sx.descripcion                                                     as 	PDI_PATROCINADO_SEXO,
	pdi_td.nombre                                                          as 	PDI_TIPODILIGENCIA,
	pdi_d.nombre                                                           as 	PDI_DILIGENCIA,
	pdi_sj.nombre                                                          as 	PDI_SITUACIONJURIDICA,
	CASE (pdi.situacionjuridica_id) 
		WHEN 11 
		THEN 'DEFENSA PENAL' 
		WHEN 12 
		THEN 'DEFENSA PENAL' 
		WHEN 13 
		THEN 'DEFENSA PENAL' 
		WHEN 14 
		THEN 'DEFENSA PENAL' 
		WHEN 15 
		THEN 'DEFENSA PENAL' 
		WHEN 16 
		THEN 'DEFENSA PENAL' 
		WHEN 17 
		THEN 'DEFENSA PENAL' 
		WHEN 18 
		THEN 'DEFENSA PENAL' 
		WHEN 19 
		THEN 'DEFENSA PENAL' 
		WHEN 20 
		THEN 'DEFENSA DE VICTIMAS' 
		WHEN 22 
		THEN 'DEFENSA PENAL' 
		WHEN 23 
		THEN 'DEFENSA PENAL' 
		WHEN 24 
		THEN 'DEFENSA DE VICTIMAS' 
		WHEN 25 
		THEN 'DEFENSA PENAL' 
