	CASE 
		when (datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)&lt;000 
		then 'Menor a 0 anios' 
		when (datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)>=00 and
		(datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)&lt;=05 
		then 'De 0 a 5 anios' 
		when (datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)>=06 and
		(datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)&lt;=11 
		then 'De 6 a 11 anios' 
		when (datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)>=12 and
		(datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)&lt;=17 
		then 'De 12 a 17 anios' 
		when (datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)>=18 and
		(datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)&lt;=29 
		then 'De 18 a 29 anios' 
		when (datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)>=30 and
		(datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)&lt;=39 
		then 'De 30 a 39 anios' 
		when (datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)>=40 and
		(datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)&lt;=49 
		then 'De 40 a 49 anios' 
		when (datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)>=50 and
		(datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)&lt;=59 
		then 'De 50 a 59 anios' 
		when (datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)>=60 and
		(datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)&lt;=69 
		then 'De 60 a 69 anios' 
		when (datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)>=70 and
		(datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)&lt;=79 
		then 'De 70 a 79 anios' 
		when (datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)>=80 and
		(datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)&lt;=89 
		then 'De 80 a 89 anios' 
		when (datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)>=90 and
		(datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)&lt;=99 
		then 'De 90 a 99 anios' 
		when (datediff(dd,pdo.fecha_nacimiento,pni.fecha_apertura)/365)>=100 
		then 'De 100 a mas anios' 
		else 'SIN ESTABLECER' 
