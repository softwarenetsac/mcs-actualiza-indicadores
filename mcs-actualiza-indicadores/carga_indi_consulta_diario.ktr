    <connection>SybaseMinjusConnection</connection>
    <sql>
SELECT 
	con_dj.nombre                                                       as 	CON_DISTRITOJUDICIAL,
	con_s.nombre                                                        as 	CON_SEDE,
	con_d.apellido_paterno+' '+con_d.apellido_materno+' '+con_d.nombres as 	CON_DEFENSOR,
	cm.nombre                                                           as 	CONSULTAMATERIA,
	ubicacion.nombre                                                    as 	CON_UBICACION,
	cdm.can_masculino                                                   as 	CON_MASCULINO,
	cdm.can_femenino                                                    as 	CON_FEMENINO,
	year(cd.fecha)                                                      as 	CON_ANIO,
	month(cd.fecha)                                                     as 	CON_MESNUM,
	(
	CASE month(cd.fecha) 
		WHEN 1 
		THEN '01-ENE' 
		WHEN 2 
		THEN '02-FEB' 
		WHEN 3 
		THEN '03-MAR' 
		WHEN 4 
		THEN '04-ABR' 
		WHEN 5 
		THEN '05-MAY' 
		WHEN 6 
		THEN '06-JUN' 
		WHEN 7 
		THEN '07-JUL' 
		WHEN 8 
		THEN '08-AGO' 
		WHEN 9 
		THEN '09-SEP' 
		WHEN 10 
		THEN '10-OCT' 
		WHEN 11 
		THEN '11-NOV' 
		WHEN 12 
		THEN '12-DIC' 
		ELSE 'SIN_MES' 
	END)                                                                as CON_MESNOM,
	cdm.can_masculino+cdm.can_femenino                                  as CONSULTA,
	cd.fecha                                                          as 	CON_FECHA,
	con_s.distritojudicial_id  AS                                           CON_DISTRITOJUDICIAL_ID,
	cd.sede_id  AS                                                          CON_SEDE_ID,
   cdm.consultamateria_id                                                  CON_consultamateria_id,
   null AS  FECHA_CIERRE

from
	dbo.v_listadopersonal con_d 
		inner join dbo.consulta_diaria cd 
		on con_d.personal_id = cd.defensor_id 
			inner join dbo.ubicacion ubicacion 
			on ubicacion.ubicacion_id = cd.ubicacion_id 
				inner join dbo.sede con_s 
				on con_s.distritojudicial_id = cd.distritojudicial_id and
				con_s.sede_id = cd.sede_id 
					inner join dbo.distritojudicial con_dj 
					on con_dj.distritojudicial_id = con_s.distritojudicial_id 
						inner join dbo.consulta_diaria_materia cdm 
						on cd.consultadiaria_id = cdm.consultadiaria_id 
							inner join dbo.consulta_materia cm 
							on cm.consultamateria_id = cdm.consultamateria_id 
where
   (cd.fecha >= ${FECHA_INICIO}) and
----	(cd.fecha >= '2021-01-01') and
	(cd.vigente ='S') 
	</sql>
